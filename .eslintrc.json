{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style" : "off",
        "no-trailing-spaces": "off",
        "no-multiple-empty-lines": "off",
        "eol-last": "off",
        "indent": "off",
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off",
        "no-new": "off",
        "class-methods-use-this": "off",
        "no-undef": "off",
        "no-return-assign": "off",
        "no-prototype-builtins": "off",
        "consistent-return": "off"
    }
}
